<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csbaic.edatope.app.mapper.BlockWorkStageMapper">


    <select id="listByBlockId" resultType="com.csbaic.edatope.app.entity.BlockWorkStage">
        SELECT * FROM sys_block_work_stage  s WHERE  s.deleted = 0 AND s.block_id = #{blockId}
    </select>

</mapper>
